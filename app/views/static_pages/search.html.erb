<% provide(:title, 'Search') %>
<div class="span7">
	<h1>Search results for <span class="timestamp">"<%= @searchkey %>"</span></h1>

	<ul class="users">
    	<li>
			<span class="sorryNoStock"><%= "No results found" unless @users != []%></span>
			<% @users.each do |user| %>	
				<li><h4><%= link_to user.name, user%></h4></li>	
			<% end %>
		</li>
	</ul>
</div>