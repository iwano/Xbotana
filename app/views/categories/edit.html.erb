<% provide(:title, "Edit category") %> 
<h1>Edit Category</h1>

<div class="row">
  <%= form_for(@category) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>

</div>