<%= form_for(@category) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

	  <%= f.label :name %>
      <%= f.text_field :name, required: true %>

      <%= f.submit "Add", class: "btn btn-large btn-primary" %>
	  <%= link_to "Cancel", categories_path, class: "btn" %>
<% end %>