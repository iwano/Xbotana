<tr>
	<td><h4><%= link_to route.id, route %></h4></td>
	<td><h4><%= link_to route.user.name, route.user %></h4></td>
	<td><h4><%= route.finished ? "finished" : "not finished" %></h4></td>
	<td><%= link_to 'Show', route, class: 'btn btn-small btn-primary' %></td>
	<% if current_user.hos? %>
		<td><%= link_to 'Delete', route, method: :delete, confirm: 'Are you sure?', class: 'btn btn-small btn-danger'%></td>
	<% end %>
</tr>